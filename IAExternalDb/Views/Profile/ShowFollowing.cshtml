@model IEnumerable<IAExternalDb.Models.User>
@{
    Layout = null;
}
<head>
    <link rel="stylesheet" href="~/Content/myCSS/List.css" />
</head>
@{
    Html.RenderPartial("Header", Session["CurrentUser"] as IAExternalDb.Models.User);
}
<div class="MainContainer">
    <p class="ListHeader">Following List</p>
    <hr>
    @foreach (var entry in Model)
    {
        <div class="ListEntry">
            <img class="entryImage" src="~/Images/ProfilePictures/@entry.ProfilePicture" />
            <p class="entryText">@entry.Fname @entry.Lname</p>
        </div>
    }
</div>